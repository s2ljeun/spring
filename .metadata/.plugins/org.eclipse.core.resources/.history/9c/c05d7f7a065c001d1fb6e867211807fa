import member.dao.MemberDAO;

@Controller
public class MemberController {

	@Autowired
	private MemberDAO memberDAO;
	
	@RequestMapping(value="/index_member.do")
	public String indexMember() {
		return "/member_index";
	}
	
	
	
}
